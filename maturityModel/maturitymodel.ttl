  msv:hasMinimumRequiredRatingOf 1 .

:M2 a dqv:Metric ;
  dqv:inDimension :Precision ;
  skos:definition "At least two decimals are provided."@en ;
  dqv:expectedDataType xsd:integer ;
  msv:hasRatingValueOf "1"^^xsd:integer .


