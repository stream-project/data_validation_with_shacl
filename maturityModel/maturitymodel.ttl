# Data cube definitions
:refMeasurement  a rdf:Property, qb:DimensionProperty ;
  rdfs:label "reference measurement set"@en ;
  rdfs:range dqv:Dimension .
:refDimension  a rdf:Property, qb:DimensionProperty ;
  rdfs:label "reference dimension"@en ;
  rdfs:range dqv:Dimension .

:dataset1 a qb:DataSet;
  rdfs:label "Data cube dataset"@en;
  rdfs:comment ""@en;
  qb:structure :dsd .

:dsd a qb:DataStructureDefinition;
  # The dimensions
  qb:component [ qb:dimension :refMeasurement; qb:order 1 ] ;
  qb:component [ qb:dimension :refDimension; qb:order 2 ] ;
  qb:component [ qb:dimension dqv:isMeasurementOf; qb:order 3 ] ;
  # The measure(s)
  qb:component [ qb:measure dqv:value ] .
